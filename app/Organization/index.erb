<div class='span12'>
  <div class="subheader">
    <h4 class='pull-left'>Organizations</h4>
  </div>
  <div id="wrapper">
    <div id="scroller">
      <table class='table'>
        <%@organizations.each do |organization| %> 
          <tr>
            <td class='list-row-desc'>
              <a href="/app/Organization/set_org?id=<%=organization.object%>" class="custom-link" data-back="/app/Gallery">
                <div class='info-grey'>
                  <span class='gallery-header'><%= organization.username %></span><br/>
                </div>
              </a>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
<script type='text/javascript'>
  basic_wrapper();
</script>
